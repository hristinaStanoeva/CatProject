{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"interface-name": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"quotemark": false,
		"trailing-comma": [true, {
			"esSpecCompliant": true
		}],
		"no-console": false,
		"object-literal-key-quotes": false,
		"arrow-parens": false,
		"max-line-length": false
	},
	"rulesDirectory": []
}
