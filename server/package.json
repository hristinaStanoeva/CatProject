{
  "name": "cat-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "start": "ntl",
    "start:js": "npm run build && node dist/server.js",
    "start:watch:dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"blue.bold,yellow.bold\" npm:start:watch:ts npm:start:watch:js",
    "start:watch:ts": "tsc -w",
    "start:watch:js": "nodemon dist/server.js",
    "start:debug": "node --inspect dist/server.js",
    "start:debug:brk": "node --inspect-brk dist/server.js",
    "start:debug:ts": "nodemon --inspect --require ts-node/register src/server.ts",
    "start:debug:brk:ts": "nodemon --inspect-brk --require ts-node/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "author": "Pi",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.13.8",
    "@types/ramda": "^0.26.8",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^24.7.1",
    "ntl": "^3.2.4",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "validator": "^10.11.0"
  }
}
